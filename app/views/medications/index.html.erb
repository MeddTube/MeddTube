<h1>Listing medications</h1>



  <% @medications.each_with_index do |medication, i| %>
  <div class = "list" >
    <%= medication.name%>
  </div>
  
  <div class = "youtube player">
    <iframe width="420" height="315" src="//www.youtube.com/embed/<%= medication.videos.take.urlcode %>" frameborder="0" allowfullscreen></iframe>
  </div>
  <% end %>





